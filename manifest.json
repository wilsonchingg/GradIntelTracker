{
  "name": "GradIntel Tracker",
  "manifest_version": 1,
  "version": "0.0.1",
  "background": {
    "scripts": ["jquery-3.2.1.min.js", "background.js"]
  },
  "browser_action": {
      "default_icon": "download.png"
  },
  "permissions": ["activeTab", "background",
    "system.storage", "system.memory", "system.display", "storage", "pageCapture",
     "https://gradintel.com/index.php/my-page"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://gradintel.com/index.php/my-page"
      ],
      "js": ["jquery-3.2.1.min.js", "foreground.js"],
      "run_at": "document_end"
    }
  ]
}
